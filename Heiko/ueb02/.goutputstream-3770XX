Mantel: Schuhe Handy Pullover
	@echo Mantel anziehen
	@touch $@

Schuhe: Struempfe Hose
	@echo Schuhe anziehen
	@touch $@

Struempfe:
	@echo Struempfe anziehen
	@touch $@

Handy: Hose
	@echo Handy anziehen 
	@touch $@

Hose: Unterhose Hemd
	@echo Hose anziehen
	@touch $@

Unterhose: 
	@echo Unterhose anziehen
	@touch $@

Pullover: Hemd
	@echo Pullover anziehen
	@touch $@

Hemd: Unterhemd
	@echo Hemd anziehen
	@touch $@

Unterhemd:
	@echo Unterhemd anziehen
	@touch $@


Nackt:
	-rm Mantel Pullover Handy Schuhe Hose Hemd Unterhemd Unterhose Struempfe -f
